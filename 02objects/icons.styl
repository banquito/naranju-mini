mixIconSrc($icon = '\f005', $color = inherit, $size = inherit) {
    content: $icon;
    color: $color;
    font-size: $size;
    font-family: icomoon;
    font-weight: normal;
    font-style: normal;
    text-decoration: inherit;
    -webkit-font-smoothing: antialiased;
}

mixIcon($icon = '\f005', $color = inherit, $size = inherit) {
  &:before {
    mixIconSrc($icon, $color, $size);
    margin-right:0.5em;
  }
}

mixIconAfter($icon = '\f005', $color = inherit, $size = inherit) {
  &:after {
    mixIconSrc($icon, $color, $size);
    margin-left:0.5em;
  }
}

mixIconSpin() {
  &:before {
    mixIconSpinSrc();
  }
}

mixIconSpinAfter() {
  &:after {
    mixIconSpinSrc();
  }
}

